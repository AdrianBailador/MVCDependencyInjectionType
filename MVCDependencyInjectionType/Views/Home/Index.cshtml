    var trasient2 = (MyObject)ViewBag.transient2;
    var scoped2 = (MyObject)ViewBag.scoped2;
    var singleton2 = (MyObject)ViewBag.singleton2;

}

<div class="text-center">

    <b>Transient 1</b> @trasient.Value
